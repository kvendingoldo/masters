set terminal svg size 600,350 enhanced fname 'times' fsize 10 butt solid
set output 'out.svg'

# Key means label...
set key inside bottom right
set xlabel 'Размер матрицы'
set ylabel 'Время (секунды)'

set xrange [10 : 100]
set yrange [0 : 13]
set ytics 0,1
set xtics 0,10

#set title 'Some Data'

plot "data.txt" using 1:2 title 'scipy' with linespoints linestyle 1 linecolor 9, "data.txt" using 1:3 title 'ℤ_2' with linespoints linestyle 1 linecolor 10, "data.txt" using 1:4 title 'ℤ_3' with linespoints linestyle 1 linecolor 7, "data.txt" using 1:5 title 'ℤ_5' with linespoints linestyle 1 linecolor 6, "data.txt" using 1:6 title 'ℤ_7' with linespoints linestyle 1 linecolor 5
